    @isTest
    public static void getPicklistFast_Call_Test() {
        
        // Set mock callout class 
        Test.setMock(HttpCalloutMock.class, new MockHttpResponseGenerator());

        Id devRecordTypeId = Schema.SObjectType.Case.getRecordTypeInfosByDeveloperName().get('Call_Case').getRecordTypeId();

		// same values as ICX_QualificationLookupHelper.js
        List<String> fieldLevelList = new List<String> { 'Type', 'Category__c', 'Domaine__c'};

		// same values as ICX_QualificationLookup.cmp
		List<Boolean> showParentWithChildrenList = new List<Boolean> { true, false, true};

		Test.startTest();
		// 'Case' as Task sObjectType is not supported for the webservice
        String result = ICX_FastQualification.getPicklistFast(devRecordTypeId, 'Case', fieldLevelList, showParentWithChildrenList);
        Test.stopTest();

        // Check
        List<ICX_FastQualification.PicklistFast> resultList = (List<ICX_FastQualification.PicklistFast>) JSON.deserialize(result, List<ICX_FastQualification.PicklistFast>.class);
        System.debug('resultList=' + resultList);
        
        Boolean findTransferLevel1Only = false;
        Boolean findProductInformationAvailabilityLevel2Only = false;
        Boolean findSalesOrderModifyLevel3Only = false;

        Boolean level1SalesShouldBeEmpty = true; // Sales
        Boolean level2ReservationShouldNotBeEmpty = false; // Sales • Reservation

        for (ICX_FastQualification.PicklistFast item : resultList) {
            System.debug('item=' + item);
            
            if ('Transfer'.equals(item.level1.value) && item.level2 == null && item.level3 == null) findTransferLevel1Only = true;
            if ('Product Information'.equals(item.level1.value) && (item.level2 != null && 'Availability'.equals(item.level2.value)) && item.level3 == null) findProductInformationAvailabilityLevel2Only = true;
            if ('Sales'.equals(item.level1.value) && (item.level2 != null && 'Order'.equals(item.level2.value)) && (item.level3 != null && 'Modify'.equals(item.level3.value))) findSalesOrderModifyLevel3Only = true;

            if ('Product Information'.equals(item.level1.value) && item.level2 == null && item.level3 == null) level1SalesShouldBeEmpty = false;
            if ('Sales'.equals(item.level1.value) && (item.level2 != null && 'Reservation'.equals(item.level2.value)) && item.level3 == null) level2ReservationShouldNotBeEmpty = true;
        }

        System.assertEquals(true, findTransferLevel1Only);
        System.assertEquals(true, findProductInformationAvailabilityLevel2Only);
        System.assertEquals(true, findSalesOrderModifyLevel3Only);

        System.assertEquals(true, level1SalesShouldBeEmpty);
        System.assertEquals(true, level2ReservationShouldNotBeEmpty);
    }

