<apex:page StandardController="Store__c" extensions="IC_Store_VFCx" cache="true" sidebar="false">

<script type="text/javascript">
   function GoToReportAllIconAction(url){
       var url = '/'+'{!IdReport_AllIconAction_Store}'+'?pv1={!Store__c.RetailStoreId__c}';
       window.parent.location.replace(url);
   }
   
   function GoToReportAppointments(url){
       var url = '/'+'{!IdReport_Appointments_Store}'+'?pv1={!Store__c.RetailStoreId__c}';
       window.parent.location.replace(url);
   }
   
   function GoToReportOutreachs(url){
       var url = '/'+'{!idReport_Outreachs_Store}'+'?pv1={!Store__c.RetailStoreId__c}';
       window.parent.location.replace(url);
   }
   
   function GoToReportAllIconActionLW(url){
       var url = '/'+'{!idReport_AllIconAction_LW_Store}'+'?pv1={!Store__c.RetailStoreId__c}';
       window.parent.location.replace(url);
   }
   
   function GoToReportAppointmentsLW(url){
       var url = '/'+'{!idReport_Appointments_LW_Store}'+'?pv1={!Store__c.RetailStoreId__c}';
       window.parent.location.replace(url);
   }
   
   function GoToReportOutreachsLW(url){
       var url = '/'+'{!idReport_Outreachs_LW_Store}'+'?pv1={!Store__c.RetailStoreId__c}';
       window.parent.location.replace(url);
   }
        
</script>

<style type="text/css">
    table
    {
        margin-left:auto;
        margin-right:auto;
    }
</style>

<apex:form >
    <table>
          <tr>
            <td>
                <apex:outputLink onclick="GoToReportAllIconAction();">
                    <analytics:reportChart reportId="{!$Label.IC_IdReport_AllIconAction_Store_Graph}" filter="[{column:'Activity.ActionType__c', operator:'equals', value:'Appointment,Email,Phone,SMS,Postal Mail,Mobile Chat'}, {column:'Activity.Owner.DefaultStore__c', operator:'equals', value:'{!StoreName}'}]" size="small"  hideOnError="false" cacheResults="false"></analytics:reportChart>
                </apex:outputLink>            
            </td>
            <td>
                <apex:outputLink onclick="GoToReportOutreachs();">
                    <analytics:reportChart reportId="{!$Label.IC_IdReport_Outreachs_Store_Graph}" filter="[{column:'Activity.ActionType__c', operator:'equals', value:'Email,Phone,SMS,Postal Mail,Mobile Chat'}, {column:'Activity.Owner.DefaultStore__c', operator:'equals', value:'{!StoreName}'}]" size="small"  hideOnError="false" cacheResults="false"></analytics:reportChart>
                </apex:outputLink>            
            </td>
            <td>
                <apex:outputLink onclick="GoToReportAppointments();">
                    <analytics:reportChart reportId="{!$Label.IC_IdReport_Appointments_Store_Graph}" filter="[{column:'Activity.IsTask', operator:'equals', value:'False'}, {column:'Activity.Owner.DefaultStore__c', operator:'equals', value:'{!StoreName}'}]" size="small"  hideOnError="false" cacheResults="false"></analytics:reportChart>
                </apex:outputLink>            
             </td>
        </tr>
        
        <tr>
            <td>
                <apex:outputLink onclick="GoToReportAllIconActionLW();">
                    <analytics:reportChart reportId="{!$Label.IC_IdReport_AllIconActionLW_Store_Graph}" filter="[{column:'Activity.ActionType__c', operator:'equals', value:'Appointment,Email,Phone,SMS,Postal Mail,Mobile Chat'}, {column:'Activity.Owner.DefaultStore__c', operator:'equals', value:'{!StoreName}'}]" size="small"  hideOnError="false" cacheResults="false"></analytics:reportChart>
                </apex:outputLink>            
            </td>
            <td>
                <apex:outputLink onclick="GoToReportOutreachsLW();">
                    <analytics:reportChart reportId="{!$Label.IC_IdReport_OutreachsLW_Store_Graph}" filter="[{column:'Activity.ActionType__c', operator:'equals', value:'Email,Phone,SMS,Postal Mail,Mobile Chat'}, {column:'Activity.Owner.DefaultStore__c', operator:'equals', value:'{!StoreName}'}]" size="small"  hideOnError="false" cacheResults="false"></analytics:reportChart>
                </apex:outputLink>            
            </td>
            <td>
                <apex:outputLink onclick="GoToReportAppointmentsLW();">
                    <analytics:reportChart reportId="{!$Label.IC_IdReport_AppointmentsLW_Store_Graph}" filter="[{column:'Activity.IsTask', operator:'equals', value:'False'}, {column:'Activity.Owner.DefaultStore__c', operator:'equals', value:'{!StoreName}'}]" size="small"  hideOnError="false" cacheResults="false"></analytics:reportChart>
                </apex:outputLink>            
             </td>
        </tr>
     </table>       
</apex:form>
</apex:page>