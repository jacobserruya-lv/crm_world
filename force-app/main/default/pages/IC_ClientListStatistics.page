<apex:page Controller="IC_ClientListStatistics_VFCx" showHeader="true">
  <style>
    .messageError { color: red; font-weight: strong;}
    .dreamLabel { font-weight: bold;}
   </style>
   
  <body> 
  <apex:form id="theContent" >
    <apex:pageBlock title="{!$Label.IC_ClientListStatistics}" >
    <apex:pageMessages />
    
        <apex:pageblocksection columns="1" title="{!pageTitle}" collapsible="false" showHeader="true" id="blocksection">
        <apex:pageBlockSection title=" Page #{!pageNumber} " columns="1">
        
	        <apex:pageBlockTable value="{!datas}" var="data" id="tableStats">
				<apex:column headerValue=""><apex:outputlink value="{!data.linkTo}" target="_blank" rendered="{!IF(data.sa <> 'TOTAL', true, false)}">View</apex:outputlink></apex:column>
				<apex:column value="{!data.sa}" headerValue="{!$Label.IC_SA}"/>
	    		<apex:column value="{!data.nbClientListMb}" headerValue="{!$Label.IC_NumberOfClients}"/>
	    		<apex:column value="{!data.nbContactedClientListMb}" headerValue="{!$Label.IC_NumberOfContactedClients}"/>
	    		<apex:column value="{!ROUND(data.perCent,0)} %" headerValue="{!$Label.IC_Percentage}"/>
	    	</apex:pageBlockTable>
		
		</apex:pageBlockSection>
        </apex:pageblocksection>

	<apex:panelGrid columns="4">
	    <apex:commandLink action="{!first}">First</apex:commandlink>
	    <apex:commandLink action="{!previous}" rendered="{!hasPrevious}">Previous</apex:commandlink>
	    <apex:commandLink action="{!next}" rendered="{!hasNext}">Next</apex:commandlink>
	    <apex:commandLink action="{!last}">Last</apex:commandlink>
    </apex:panelGrid>



    </apex:pageBlock>
    
    

  </apex:form>   
  </body>

</apex:page>