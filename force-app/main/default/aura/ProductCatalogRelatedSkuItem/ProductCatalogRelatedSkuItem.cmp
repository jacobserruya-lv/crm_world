<aura:component implements="flexipage:availableForAllPageTypes,force:appHostable" controller="Ctrl_ProductCatalog" access="global">
    <aura:attribute name="relatedSku" type="Object" />
    <a onclick="{!c.optionClick}">
    	<img src="{!(
                not(empty(v.relatedSku.Value))
                    ? v.relatedSku.Value + '?wid=100&amp;hei=100'
                    : 'data:img/png;base64,'
            )}"
             width="100"
             height="100"
        />
    </a>
</aura:component>