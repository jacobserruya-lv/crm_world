<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>GlobalStatusFormula__c</fullName>
    <externalId>false</externalId>
    <formula>IF(nbOpenShipping__c &gt; 0, &quot;OPEN&quot;, IF(nbReceivedShipping__c &gt; 0, &quot;READY FOR PICKUP&quot;, IF(nbCompleteShipping__c &gt; 0, &quot;COMPLETE&quot;, &quot;CANCELLED&quot;) ) )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Global Status Formula</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
