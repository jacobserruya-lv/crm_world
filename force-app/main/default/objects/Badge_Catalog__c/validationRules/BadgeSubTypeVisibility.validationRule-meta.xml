<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>BadgeSubTypeVisibility</fullName>
    <active>true</active>
    <errorConditionFormula>NOT ISPICKVAL(badgeType__c , &apos;Global Engagement&apos;) &amp;&amp; NOT ISBLANK(TEXT  (BadgeSubType__c))</errorConditionFormula>
    <errorDisplayField>BadgeSubType__c</errorDisplayField>
    <errorMessage>BadgeSubType__c cannot be set when BadgeType__c is not Global Engagement</errorMessage>
</ValidationRule>
