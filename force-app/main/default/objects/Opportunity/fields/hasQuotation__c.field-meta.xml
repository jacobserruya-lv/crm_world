<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>hasQuotation__c</fullName>
    <description>Indicates whether or not the brief requires quotation (depends on the opportunity category and the order type)</description>
    <externalId>false</externalId>
    <formula>OR(TEXT(SPO_ProductCategory__c)  &lt;&gt; &quot;Leather goods&quot;,  TEXT(SPO_OrderType__c)  &lt;&gt; &quot;MTO on Catalog (Hardsided)&quot;)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>hasQuotation</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
