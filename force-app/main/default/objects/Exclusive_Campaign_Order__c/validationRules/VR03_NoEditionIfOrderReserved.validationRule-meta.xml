<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>VR03_NoEditionIfOrderReserved</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
PRIORVALUE(Reserved__c ) = true,
NOT(ISNEW()),
OR(
$Profile.Name = &quot;ICON_SA&quot;,
$Profile.Name = &quot;ICON_SA_EVERYWHERE&quot;,
$Profile.Name = &quot;ICONiCS_SA&quot;
)
)</errorConditionFormula>
    <errorMessage>You are not allowed to modify a reserved order</errorMessage>
</ValidationRule>
