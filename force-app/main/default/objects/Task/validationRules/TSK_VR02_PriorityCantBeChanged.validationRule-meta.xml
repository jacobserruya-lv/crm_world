<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>TSK_VR02_PriorityCantBeChanged</fullName>
    <active>true</active>
    <description>Priority Field can&apos;t be changed.</description>
    <errorConditionFormula>NOT($User.BypassVR__c ) 
&amp;&amp;
$Profile.Name  &lt;&gt; &quot;ICON_Interface DREAM&quot;
&amp;&amp;
ISCHANGED(Priority)
&amp;&amp;
RecordType.Name =&quot;DREAM Contact&quot;</errorConditionFormula>
    <errorDisplayField>Priority</errorDisplayField>
    <errorMessage>&quot;Priority&quot; field can&apos;t be edit for action coming from DREAM.</errorMessage>
</ValidationRule>
