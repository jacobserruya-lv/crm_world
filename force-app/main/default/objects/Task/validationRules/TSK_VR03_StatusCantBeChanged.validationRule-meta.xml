<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>TSK_VR03_StatusCantBeChanged</fullName>
    <active>true</active>
    <description>Status Field can&apos;t be changed.</description>
    <errorConditionFormula>NOT($User.BypassVR__c ) 
&amp;&amp;
$Profile.Name  &lt;&gt; &quot;ICON_Interface DREAM&quot;
&amp;&amp;
ISCHANGED(Status)
&amp;&amp;
RecordType.Name =&quot;DREAM Contact&quot;</errorConditionFormula>
    <errorDisplayField>Status</errorDisplayField>
    <errorMessage>&quot;Status&quot; field can&apos;t be edit for action coming from DREAM.</errorMessage>
</ValidationRule>
