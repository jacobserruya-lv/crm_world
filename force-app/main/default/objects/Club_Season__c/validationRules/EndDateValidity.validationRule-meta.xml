<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>EndDateValidity</fullName>
    <active>true</active>
    <errorConditionFormula>StartDate__c &gt; EndDate__c</errorConditionFormula>
    <errorDisplayField>EndDate__c</errorDisplayField>
    <errorMessage>EndDate__c cannot be smaller than StartDate__c</errorMessage>
</ValidationRule>
