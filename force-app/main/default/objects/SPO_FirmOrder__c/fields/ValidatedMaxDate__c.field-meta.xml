<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ValidatedMaxDate__c</fullName>
    <description>For Now Yours app or ECO website, &apos;ETL Status&apos; field is set to &apos;V&apos; in creation. This status changed when this date is coming (Time-Dependant Workflow)</description>
    <externalId>false</externalId>
    <formula>IF (
  NOT(ISBLANK(ProductCatalogue__c)),
  CreatedDate + ProductCatalogue__r.LeadTimeApproval__c,
  CreatedDate
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Validated Max Date</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>DateTime</type>
</CustomField>
