<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>UnitaryPrice__c</fullName>
    <description>Get Retail price what ever the Order/FirmOrder type. Used by MIY-476&amp; MIY-481 (SPO_FirmOrderTile.cmp)</description>
    <externalId>false</externalId>
    <formula>IF (SPO_BriefName__r.SPO_UnitRetailPriceQuotation__c &lt;&gt; 0,
SPO_BriefName__r.SPO_UnitRetailPriceQuotation__c,
UnitRetailPrice__c )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Unitary Price</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
