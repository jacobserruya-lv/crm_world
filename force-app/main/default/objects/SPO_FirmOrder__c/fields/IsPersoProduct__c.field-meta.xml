<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>IsPersoProduct__c</fullName>
    <description>If the custom product contains macro-component (For JDE to send the product to the production or to be get in warehouse)</description>
    <externalId>false</externalId>
    <formula>IF (
  AND (
    LEFT(ProductSKU__c, 1) = &apos;P&apos;,
    NOT(ISBLANK(FluidRecipeId__c))
  ),
  1,
  0
)</formula>
    <label>Is Perso Product</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
