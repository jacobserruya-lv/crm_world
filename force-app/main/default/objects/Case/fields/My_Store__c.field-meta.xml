<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>My_Store__c</fullName>
    <description>Checks whether the connected user&apos;s store and the store completed in the request are the same</description>
    <externalId>false</externalId>
    <formula>Store__r.RetailStoreId__c  = TEXT($User.DefaultStore__c )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>My Store</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
