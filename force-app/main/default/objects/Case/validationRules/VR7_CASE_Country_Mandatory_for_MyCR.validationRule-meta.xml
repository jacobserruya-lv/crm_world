<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>VR7_CASE_Country_Mandatory_for_MyCR</fullName>
    <active>false</active>
    <description>Country is mandatory for MyCR</description>
    <errorConditionFormula>AND( 
NOT( $User.BypassVR__c ), 
ISPICKVAL( Resolution__c , &quot;Request to Store&quot;), 
ISPICKVAL( Country__c, &quot;&quot; ))</errorConditionFormula>
    <errorMessage>Please add &quot;Country&quot; before sending MyCR</errorMessage>
</ValidationRule>
